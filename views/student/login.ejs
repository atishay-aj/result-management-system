<a href="/" class="btn btn-info" style="margin-top:5px;">Back</a>
<h3>Student Login</h3>
<h5 style="text-align:center;color: red;">
  <%= error %>
</h5>
<div class="teachLogClass">
  <form method="POST" class="teachFormClass">
    <div class="mb-3">
      <label class="form-label">Roll no : </label>
      <input type="number" name="roll" class="form-control">
    </div>
    <div class="mb-3">
      <label class="form-label">Date of Birth : </label>
      <input type="date" name="dob" class="form-control">
    </div>
    <a><button type="submit" class="btn btn-primary">Submit</button></a>
  </form>
</div>